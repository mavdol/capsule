{
  "name": "@capsule-run/sdk",
  "version": "0.6.0",
  "description": "Capsule JavaScript SDK - run AI agent tasks in secure WASM sandboxes",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "homepage": "https://github.com/mavdol/capsule",
  "repository": {
    "type": "git",
    "url": "https://github.com/mavdol/capsule.git"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./capsule/app.js": {
      "types": "./dist/app.d.ts",
      "import": "./dist/app.js"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "webassembly",
    "agents",
    "isolation",
    "llm",
    "ai"
  ],
  "license": "Apache-2.0",
  "dependencies": {
    "@bytecodealliance/jco": "^1.0.0",
    "buffer": "^6.0.3",
    "esbuild": "^0.27.2",
    "events": "^3.3.0",
    "path-browserify": "^1.0.1",
    "readable-stream": "^4.7.0",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "@types/events": "^3.0.3",
    "@types/path-browserify": "^1.0.3",
    "@types/readable-stream": "^4.0.18"
  },
  "peerDependencies": {
    "@types/node": ">=18"
  },
  "peerDependenciesMeta": {
    "@types/node": {
      "optional": true
    }
  }
}
