{
  "name": "@capsule-run/sdk",
  "version": "0.2.2",
  "description": "Capsule JavaScript SDK - run AI agent tasks in secure WASM sandboxes",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "homepage": "https://github.com/mavdol/capsule",
  "repository": {
    "type": "git",
    "url": "https://github.com/mavdol/capsule.git"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./capsule/app.js": {
      "types": "./dist/app.d.ts",
      "import": "./dist/app.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "webassembly",
    "agents",
    "isolation",
    "llm",
    "ai"
  ],
  "license": "Apache-2.0",
  "dependencies": {
    "@bytecodealliance/jco": "^1.0.0",
    "esbuild": "^0.27.2"
  },
  "devDependencies": {
    "typescript": "^5.9.3"
  }
}
